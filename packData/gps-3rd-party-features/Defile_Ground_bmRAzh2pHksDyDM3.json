              "source": "if(game.user.id !== event.user.id) return;\n\nlet effectData = event.data.token.actor.appliedEffects.find(e => e.name === \"Defile Ground\");\n\nif(!effectData) {\n  effectData = [{\n        \"disabled\": false,\n        \"name\": \"Defile Ground\",\n        \"img\": \"icons/magic/nature/root-vine-entangled-humanoid.webp\",\n        \"changes\": [\n          { \n            \"key\": \"flags.midi-qol.onUseMacroName\", \n            \"mode\": CONST.ACTIVE_EFFECT_MODES.CUSTOM, \n            \"value\": \"function.game.gps.defileGround,isDamaged\", \n            \"priority\": 20 \n          }\n        ],\n        \"transfer\": false\n  }];\n\nawait MidiQOL.socket().executeAsGM(\"createEffects\", { actorUuid: event.data.token.actor.uuid, effects: effectData });\n}"
