    "dae": {
      "macro": {
        "name": "Wickerbone Behemoth",
        "img": "icons/creatures/magical/spirit-undead-ghost-tan-teal.webp",
        "type": "script",
        "scope": "global",
        "command": "if(args[0] === \"on\") {\n    await token.document.update({\n        \"texture.src\": \"assets/pcs/demon_tree_lesser_02_hi.webp\",\n        \"texture.scaleX\": 1.5,\n        \"texture.scaleY\": 1.5\n    }); // Change transformed token here\n\n    let itemData = actor.items.getName(\"Club\");\n    await itemData.update({\"system.equipped\" : true});\n    const weaponItem = actor.getEmbeddedDocument(\"Item\", itemData.id);\n    const weaponCopy = duplicate(weaponItem);\n    await DAE.setFlag(actor, \"shillelagh\", {\n        id: itemData.id,\n        name: weaponItem.name,\n        damage: weaponItem.system.damage.parts[0][0],\n        ability: weaponItem.system.ability,\n        magical: getProperty(weaponItem, \"system.properties.mgc\") || false,\n    });\n    const damage = weaponCopy.system.damage.parts[0][0];\n    const targetAbilities = actor.system.abilities;\n    weaponCopy.system.damage.parts[0][0] = damage.replace(/1d(4|6)/g, \"1d8\");\n    if (targetAbilities.wis.value > targetAbilities.str.value) weaponCopy.system.ability = \"wis\";\n    weaponCopy.name = weaponItem.name + \" [Shillelagh]\";\n    setProperty(weaponCopy, \"system.properties.mgc\", true);\n    await actor.updateEmbeddedDocuments(\"Item\", [weaponCopy]);\n}\n\nif(args[0] === \"off\") {\n    await token.document.update({\"texture.src\" : \"tokenizer/pc-images/cait.token.webp?1714091336458\",\n        \"texture.scaleX\": 1,\n        \"texture.scaleY\": 1\n    }); // Change origin token here\n\n    const flag = DAE.getFlag(actor, \"shillelagh\");\n    const weaponItem = actor.getEmbeddedDocument(\"Item\", flag.id);\n    const weaponCopy = duplicate(weaponItem);\n    weaponCopy.system.damage.parts[0][0] = flag.damage;\n    weaponCopy.system.ability = flag.ability;\n    weaponCopy.name = flag.name;\n    setProperty(weaponCopy, \"system.properties.mgc\", flag.magical);\n    await actor.updateEmbeddedDocuments(\"Item\", [weaponCopy]);\n    await DAE.unsetFlag(actor, \"shillelagh\");\n    let itemData = actor.items.getName(\"Club\");\n    await itemData.update({\"system.equipped\" : false});\n    await actor.unsetFlag(\"midi-qol\", \"wickerboneAccDamage\");\n}\n\nif(args[0].macroPass === \"isDamaged\") {\n    async function wait(ms) { return new Promise(resolve => { setTimeout(resolve, ms); }); }\n    let gameVersion = parseInt(game.system.version.split('.')[0], 10);\n    const rangeCheck = MidiQOL.findNearby(null, token, 5, { includeToken: false });\n    const withinRange = rangeCheck.filter(i => i.document.disposition !== token.document.disposition);\n    if (withinRange.length === 0) return;\n    let itemImage = macroItem.img;\n    for(target of withinRange) {\n        let targetUuid = target.document.uuid;\n        let diceTotal = (actor.system.details.level < 10) ? '1d4' : '2d4';\n\n        const tempEffectData = [{\n        \"icon\": `${itemImage}`,\n        \"name\": \"Wickerbone Behemoth Splinter\",\n        \"changes\": [],\n        \"origin\": actor.uuid,\n        \"disabled\": false,\n        \"duration\": {\n            \"rounds\": 1\n        },\n        \"flags\": {\n            \"effectmacro\": {\n                \"onCreate\": {\n                    \"script\": `\n                            const itemData = {\n                            name: \"Wickerbone Behemoth Splinter\",\n                            type: \"feat\",\n                                img: '${itemImage}',\n                            effects: [],\n                            flags: {\n                                \"midi-qol\": {\n                                    noProvokeReaction: true,\n                                    onUseMacroName: null,\n                                    forceCEOff: true\n                                },\n                                \"midiProperties\": {\n                                    nodam: true\n                                },\n                            },\n                            system: {\n                                equipped: true,\n                                actionType: \"attack\",\n                                damage: { parts: [['${diceTotal}', 'piercing']] },\n                                components: { concentration: false, material: false, ritual: false, somatic: false, value: \"\", vocal: false },\n                                duration: { units: \"inst\", value: undefined },\n                                properties: {mgc: true}\n                            },\n                            };\n                            const itemUpdate = new CONFIG.Item.documentClass(itemData, {parent: actor});\n                            const options = { showFullCard: false, createWorkflow: true, versatile: false, configureDialog: false, targetUuids: ['${targetUuid}'], workflowOptions: {autoRollDamage: 'always', autoFastDamage: true} };\n                            await MidiQOL.completeItemUse(itemUpdate, {}, options);\n                    `\n                }\n            }\n        }\n    }];\n\n    await MidiQOL.socket().executeAsGM(\"createEffects\", { actorUuid: actor.uuid, effects: tempEffectData });\n    await wait(100);\n    let tempEffectRemove;\n    if(gameVersion >= 3) {\n        tempEffectRemove = actor.appliedEffects.find(i => i.name.toLowerCase() === \"wickerbone behemoth splinter\");\n    }\n    else {\n        tempEffectRemove = actor.effects.getName(\"Wickerbone Behemoth Splinter\");\n    }\n    await MidiQOL.socket().executeAsGM(\"removeEffects\", { actorUuid: actor.uuid, effects: [tempEffectRemove.id] });\n    }\n\n    let damageReceived = await actor.getFlag(\"midi-qol\", \"wickerboneAccDamage\");\n    if(!damageReceived) await actor.setFlag(\"midi-qol\", \"wickerboneAccDamage\", workflow.damageTotal);\n    else await actor.setFlag(\"midi-qol\", \"wickerboneAccDamage\", damageReceived + workflow.damageTotal);\n}",
