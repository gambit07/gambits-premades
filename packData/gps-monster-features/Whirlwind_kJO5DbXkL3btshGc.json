    "dae": {
      "macro": {
        "name": "Whirlwind",
        "img": "icons/magic/air/wind-tornado-cyclone-white.webp",
        "type": "script",
        "scope": "global",
        "command": "if(args[0].macroPass === \"postSave\") {\n    let targets = workflow.failedSaves;\n    let gmUser = game.gps.getPrimaryGM();\n    if(targets.size === 0) return;\n    for(let target of targets) {\n      const directions = [\"North\", \"South\", \"East\", \"West\", \"Northwest\", \"Northeast\", \"Southwest\", \"Southeast\"];\n      const randomDirection = directions[Math.floor(Math.random() * directions.length)];\n      await game.gps.socket.executeAsGM(\"moveTokenByCardinal\", {targetUuid: target.document.uuid, distance: 20, direction: randomDirection});\n      await game.gps.socket.executeAsUser(\"gmToggleStatus\", gmUser, {tokenUuid: `${target.document.uuid}`, status: \"prone\", active: true });\n    }\n}",
